<%= render 'shared/errors', obj: @organization %>

<%= form_with(model: @organization, local: true) do |f| %>
 
  <div class="form-group">
    <%= f.label :name %><br/>
    <%= f.text_field :name, class: "form-control" %>
  </div>
   
  <div class="form-group">
    <%= f.submit class: "btn btn-primary" %>
  </div>
   
  <% end %>